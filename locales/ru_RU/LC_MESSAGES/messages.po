msgid ""
msgstr ""
"Content-Type: text/plain; charset=UTF-8\n"

msgid "Enter YouTube video URL (Press ENTER to exit): "
msgstr "Введите URL-адрес видео на YouTube (нажмите ENTER для выхода): "

msgid "Available download options:"
msgstr "Доступные варианты загрузки:"

msgid "Resolution: %(resolution)s, Format: %(mime_type)s, Type: %(type)s, Quality: %(abr)s"
msgstr "Разрешение: %(resolution)s, Формат: %(mime_type)s, Тип: %(type)s, Качество: %(abr)s"

msgid "Starting download of file: %(title)s.%(file_extension)s"
msgstr "Начало загрузки файла: %(title)s.%(file_extension)s"

msgid "Video successfully downloaded!"
msgstr "Видео успешно загружено!"

msgid "Error: Invalid YouTube video URL."
msgstr "Ошибка: Некорректный URL-адрес видео на YouTube."

msgid "An error occurred while downloading the video: %(exception_type)s: %(exception_message)s"
msgstr "Произошла ошибка во время загрузки видео: %(exception_type)s: %(exception_message)s"

msgid "Linux terminal utility v.0.0.1 - that downloads videos from YouTube using a provided link."
msgstr "Утилита для терминала Linux v.0.0.1 - для загрузки видео с YouTube по предоставленной ссылке."

msgid "Uploaded: %(percentage).2f%% | Remaining Time: %(formatted_remaining_time)s (upload time: %(formatted_elapsed_time)s)"
msgstr "Загружено: %(percentage).2f%% | Оставшееся время: %(formatted_remaining_time)s (время загрузки: %(formatted_elapsed_time)s)"

msgid "usage: "
msgstr "использование: "

msgid "options:"
msgstr "опции:"

msgid "URL - link to the YouTube video"
msgstr "URL - ссылка на видео с YouTube"

msgid "usage: y-tube-dl [-h] [--URL URL]; by default, it starts in interactive mode with limited functionality."
msgstr "используйте: y-tube-dl [-h] [--URL URL]; по умолчанию запускается в интерактивном режиме с ограниченным функционалом."

